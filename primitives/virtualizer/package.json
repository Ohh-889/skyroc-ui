{
  "name": "@skyroc/virtualizer",
  "type": "module",
  "version": "0.0.1",
  "description": "Headless virtualizer components for efficient rendering of large lists and grids",
  "author": {
    "name": "Ohh",
    "email": "1509326266@qq.com",
    "url": "https://github.com/Ohh-889"
  },
  "license": "MIT",
  "homepage": "https://github.com/Ohh-889/skyroc-ui/tree/master/primitives/virtualizer",
  "keywords": [
    "virtualizer",
    "virtual-list",
    "virtual-scroll",
    "react",
    "typescript",
    "headless",
    "skyroc"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      }
    }
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "README.zh.md",
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint src/**/*.ts --fix"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0 || ^19.0.0-rc"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.6"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}

