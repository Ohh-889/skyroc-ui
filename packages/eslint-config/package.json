{
  "name": "@skyroc/eslint-config",
  "type": "module",
  "version": "0.0.1",
  "description": "Skyroc's ESLint flat config for Next.js, React, React Native, and Vue",
  "author": "Skyroc Team",
  "license": "MIT",
  "homepage": "https://github.com/skyroc/skyroc-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/skyroc/skyroc-ui.git",
    "directory": "packages/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-react-native",
    "eslint-config-next"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        }
      }
    }
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "tsdown",
    "lint": "eslint src/**/*.ts --fix",
    "stub": "tsdown --watch"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": ">=15.0.0",
    "@unocss/eslint-config": ">=0.60.0",
    "eslint": ">=9.10.0",
    "eslint-plugin-erasable-syntax-only": ">=0.4.0",
    "eslint-plugin-react": ">=7.30.0",
    "eslint-plugin-react-hooks": ">=7.0.0",
    "eslint-plugin-react-native": ">=4.0.0",
    "eslint-plugin-react-refresh": ">=0.4.0",
    "eslint-plugin-tailwindcss": ">=3.17.0",
    "eslint-plugin-vue": ">=9.19.0",
    "typescript": ">=5.0.0",
    "vue-eslint-parser": ">=9.3.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@unocss/eslint-config": {
      "optional": true
    },
    "eslint-plugin-erasable-syntax-only": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-native": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-tailwindcss": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@eslint-community/eslint-plugin-eslint-comments": "catalog:",
    "@eslint/js": "catalog:",
    "@eslint/markdown": "catalog:",
    "@stylistic/eslint-plugin": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@vitest/eslint-plugin": "catalog:",
    "eslint-config-flat-gitignore": "catalog:",
    "eslint-flat-config-utils": "catalog:",
    "eslint-mdx": "catalog:",
    "eslint-merge-processors": "catalog:",
    "eslint-parser-plain": "catalog:",
    "eslint-plugin-antfu": "catalog:",
    "eslint-plugin-command": "catalog:",
    "eslint-plugin-import-lite": "catalog:",
    "eslint-plugin-import-x": "catalog:",
    "eslint-plugin-jsdoc": "catalog:",
    "eslint-plugin-jsonc": "catalog:",
    "eslint-plugin-mdx": "catalog:",
    "eslint-plugin-n": "catalog:",
    "eslint-plugin-no-only-tests": "catalog:",
    "eslint-plugin-perfectionist": "catalog:",
    "eslint-plugin-pnpm": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-regexp": "catalog:",
    "eslint-plugin-toml": "catalog:",
    "eslint-plugin-unicorn": "catalog:",
    "eslint-plugin-unused-imports": "catalog:",
    "eslint-plugin-yml": "catalog:",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "catalog:",
    "local-pkg": "^1.1.2",
    "prompts": "^2.4.2",
    "toml-eslint-parser": "catalog:",
    "yaml-eslint-parser": "catalog:"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "catalog:",
    "@types/eslint": "^9.6.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/node": "catalog:",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-config": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-erasable-syntax-only": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-react-native": "catalog:",
    "eslint-plugin-react-refresh": "catalog:",
    "eslint-plugin-tailwindcss": "catalog:",
    "eslint-plugin-vue": "catalog:",
    "eslint-processor-vue-blocks": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vue-eslint-parser": "catalog:"
  }
}
